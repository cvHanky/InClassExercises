<Window x:Class="Pr28_Tusindfryd_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pr28_Tusindfryd_GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="300">
    <Grid Margin="20">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="1" VerticalAlignment="Center" Height="auto" Width="auto" Click="Button_Click">Start Produktion</Button>
        <Button Grid.Row="2" VerticalAlignment="Center" Height="auto" Width="auto">Gennemse Produktion</Button>
        <Button Grid.Row="3" VerticalAlignment="Center" Height="auto" Width="auto">Afslut Produktion</Button>
        <Button Grid.Row="5" VerticalAlignment="Center" Height="auto" Width="auto">Se blomstersorter</Button>
    </Grid>
</Window>
